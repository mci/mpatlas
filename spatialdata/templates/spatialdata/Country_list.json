{
    "total_count": {% if paginator %}{{ paginator.count }}{% else %}{{ country_list|length }}{% endif %},
    "total_pages": {% if paginator %}{{ paginator.num_pages }}{% else %}1{% endif %},
    "current_page": {% if page_obj %}{{ page_obj.number }}{% else %}1{% endif %},
    "countries": [{% for country in country_list %}
        {
            "name": "{{ country.name }}",
            "iso3code": "{{ country.iso3code }}", 
            "marine_area": "{{ country.marine_area }}",
            "designated_mpa_area": "{{ country.designated_mpa_area }}",
            "designated_mpa_percent": "{{ country.designated_mpa_percent }}",
            "implemented_mpa_area": "{{ country.implemented_mpa_area }}",
            "implemented_mpa_percent": "{{ country.implemented_mpa_percent }}",
            "unimplemented_mpa_area": "{{ country.unimplemented_mpa_area }}",
            "unimplemented_mpa_percent": "{{ country.unimplemented_mpa_percent }}",
            "designated_highmpa_area": "{{ country.designated_highmpa_area }}",
            "designated_highmpa_percent": "{{ country.designated_highmpa_percent }}",
            "implemented_highmpa_area": "{{ country.implemented_highmpa_area }}",
            "implemented_highmpa_percent": "{{ country.implemented_highmpa_percent }}",
            "unimplemented_highmpa_area": "{{ country.unimplemented_highmpa_area }}",
            "unimplemented_highmpa_percent": "{{ country.unimplemented_highmpa_percent }}",
            "proposed_mpa_area": "{{ country.proposed_mpa_area }}",
            "proposed_mpa_percent": "{{ country.proposed_mpa_percent }}",
            "url": "{{ country.get_absolute_url }}"
        }{% if not forloop.last %},{% endif %}{% endfor %}
    ]
}
